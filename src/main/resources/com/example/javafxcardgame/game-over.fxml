<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.javafxcardgame.MainController"
      alignment="CENTER">
    <Text
        text="Game Over!"
        style="-fx-font-size:40px"
        />
    <Text
        fx:id="endMoney"
        text="Money:"
        />
    <Text
        fx:id="roundsPlayed"
        text="Rounds played:"
        />
    <Text
        fx:id="roundsWon"
        text="Rounds won:"
        />
    <Text
        fx:id="roundsLost"
        text="Rounds lost:"
        />
    <Button
        fx:id="resetButton"
        text="Try Again?"
        />
</VBox>
